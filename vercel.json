{
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "^(?!www\\.)[a-zA-Z0-9-]+\\.bagthatthangup\\.com$" }
      ],
      "destination": "/tenant/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

