{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "echo 'Static build complete'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "^(?<sub>[a-z0-9-]+)\\.bagthatthangup\\.com$" }
      ],
      "destination": "/tenant/index.html"
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "^(?<sub>[a-z0-9-]+)\\.bagthatthangup\\.com$" }
      ],
      "destination": "/tenant/$1"
    },
    {
      "source": "/onboarding",
      "destination": "/onboarding/index.html"
    },
    {
      "source": "/success",
      "destination": "/success/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
