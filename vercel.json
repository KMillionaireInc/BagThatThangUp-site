{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "^[^.]+\\.bagthatthangup\\.com$" }
      ],
      "destination": "/tenant/index.html"
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "^[^.]+\\.bagthatthangup\\.com$" }
      ],
      "destination": "/tenant/index.html"
    },
    { "source": "/onboarding", "destination": "/onboarding/index.html" },
    { "source": "/onboarding/(.*)", "destination": "/onboarding/index.html" },
    { "source": "/success", "destination": "/success/index.html" },
    { "source": "/success/(.*)", "destination": "/success/index.html" },
    { "source": "/", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
