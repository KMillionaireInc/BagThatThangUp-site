{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/stripe-success",
      "destination": "/stripe-success.html"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.bagthatthangup.com" }],
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "*.bagthatthangup.com" }],
      "destination": "/tenant/index.html"
    }
  ]
}
