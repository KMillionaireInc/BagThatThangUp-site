{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/success(.*)", "destination": "/success/index.html" },
    { "source": "/onboarding(.*)", "destination": "/onboarding/index.html" },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "^(?<sub>[a-z0-9-]+)\\.bagthatthangup\\.com$" }],
      "destination": "/tenant/index.html"
    },
    { "source": "/", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
