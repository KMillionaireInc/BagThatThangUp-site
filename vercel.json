{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "^(?<subdomain>[a-z0-9-]+)\\.bagthatthangup\\.com$" }
      ],
      "destination": "/tenant/$1"
    },
    {
      "source": "/onboarding(.*)",
      "destination": "/onboarding/index.html"
    },
    {
      "source": "/success(.*)",
      "destination": "/success/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
