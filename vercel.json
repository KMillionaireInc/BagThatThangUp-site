{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [],
  "rewrites": [
    {
      "source": "/onboarding/:path*/",
      "destination": "/onboarding/:path*/index.html"
    },
    {
      "source": "/onboarding/:path*",
      "destination": "/onboarding/:path*/index.html"
    },
    {
      "has": [{ "type": "host", "value": "bagthatthangup.com" }],
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/tenant/index.html"
    }
  ]
}
