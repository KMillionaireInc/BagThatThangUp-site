{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/onboarding", "destination": "/onboarding/index.html" },
    { "source": "/onboarding/(.*)", "destination": "/onboarding/index.html" },
    { "source": "/success", "destination": "/success/index.html" },
    { "source": "/success/(.*)", "destination": "/success/index.html" },
    { "source": "/tenant", "destination": "/public/tenant/index.html" },
    { "source": "/tenant/(.*)", "destination": "/public/tenant/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
