{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/success", "destination": "/success/index.html" },
    { "source": "/onboarding", "destination": "/onboarding/index.html" },
    { "source": "/", "has": [{ "type": "host", "value": "*.bagthatthangup.com" }], "destination": "/tenant/index.html" },
    { "source": "/", "destination": "/onboarding/index.html" }
  ]
}
