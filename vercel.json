"routes": [
  { "src": "^/onboarding$", "dest": "/onboarding/index.html" },
  { "src": "^/success$", "dest": "/success/index.html" },

  {
    "src": "^/$",
    "has": [
      {
        "type": "host",
        "value": "^(?!www\\.)[a-z0-9-]+\\.bagthatthangup\\.com$"
      }
    ],
    "dest": "/tenant/index.html"
  },

  {
    "src": "^/$",
    "has": [
      { "type": "host", "value": "^(www\\.)?bagthatthangup\\.com$" }
    ],
    "dest": "/index.html"
  },

  { "src": "/(.*)", "dest": "/index.html" }
]

