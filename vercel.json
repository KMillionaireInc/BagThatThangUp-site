{
  "rewrites": [
    { "source": "/success", "destination": "/success.html" },
    { "source": "/onboarding", "destination": "/onboarding.html" },
    { "source": "/namecheck", "destination": "/namecheck.html" },

    {
      "source": "/",
      "has": [{ "type": "host", "value": "^(?<sub>[a-z0-9-]+)\\.bagthatthangup\\.com$" }],
      "destination": "/public/tenant/index.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "^(?<sub>[a-z0-9-]+)\\.bagthatthangup\\.com$" }],
      "destination": "/public/tenant/$1"
    }
  ]
}
