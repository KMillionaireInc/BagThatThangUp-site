{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "onboarding/index.html", "use": "@vercel/static" },
    { "src": "success/index.html", "use": "@vercel/static" },
    { "src": "tenant/index.html", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/onboarding", "destination": "/onboarding/index.html" },
    { "source": "/success", "destination": "/success/index.html" },
    { "source": "/tenant", "destination": "/tenant/index.html" },
    { "source": "/tenant/(.*)", "destination": "/tenant/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(?!bagthatthangup\\.com$)(.*)\\.bagthatthangup\\.com$"
        }
      ],
      "destination": "/tenant/index.html",
      "permanent": false
    }
  ]
}
