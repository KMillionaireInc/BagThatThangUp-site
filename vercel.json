{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/success", "destination": "/success.html" },
    { "source": "/onboarding", "destination": "/onboarding.html" },
    { "source": "/namecheck", "destination": "/namecheck.html" },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "([^\\.]+)\\.bagthatthangup\\.com" }
      ],
      "destination": "/tenant/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
